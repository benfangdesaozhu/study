{
    "presets": [
        "@babel/preset-env",
        "@vue/cli-plugin-babel/preset"
    ],
    "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": true,
            "version": "7.0.0-beta.0"
          }
        ]
      ],
      "env": {
        "development" : {
          "compact": false
        }
      }
}